#!/bin/bash

if [ $# -ne 1 ]; 
    then echo "Specify '2' or '3' to select python version"
    exit
fi

if [ $1 == "3" ]; then
    cp $ZSH/python/python3.sh $ZSH/python/python.sh
    cp $ZSH/python/pip3.sh $ZSH/python/pip.sh
else
    cp $ZSH/python/python2.sh $ZSH/python/python.sh
    cp $ZSH/python/pip2.sh $ZSH/python/pip.sh
fi
